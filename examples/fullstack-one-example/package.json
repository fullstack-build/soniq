{
  "name": "fullstack.one-example",
  "version": "0.5.11",
  "description": "Example project: how to use fullstack.one",
  "private": true,
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "main": "index.ts",
  "scripts": {
    "start": "ts-node .",
    "debug": "FULLSTACK_ONE_DEV=true ts-node-dev --notify=false --inspect=9229 .",
    "dev": "FULLSTACK_ONE_DEV=true ts-node-dev --notify=false .",
    "dev-pure": "FULLSTACK_ONE_DEV=true ts-node .",
    "migrate-db": "one migrate-db",
    "generate-ts": "one generate-ts --output types/generated.ts",
    "tslint": "tslint './server/**/*.ts?(x)' -e '**/node_modules/**' --fix",
    "test": "echo \"No tests specified :(\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fullstack-build/fullstack.one-example.git"
  },
  "keywords": [
    "fullstack.one",
    "example"
  ],
  "author": "Eugene <opensource@terehov.de> (http://fullstack.build)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fullstack-build/fullstack.one-example/issues"
  },
  "homepage": "https://github.com/fullstack-build/fullstack.one-example#readme",
  "devDependencies": {
    "@types/dotenv": "^4.0.2",
    "@types/node": "^8.0.57",
    "ts-node": "^4.1.0",
    "ts-node-dev": "^1.0.0-pre.26",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.4.2",
    "typescript": "^3.1.1"
  },
  "dependencies": {
    "@fullstack-one/auth": "^0.5.9",
    "@fullstack-one/auth-fb-token": "^0.5.9",
    "@fullstack-one/auto-migrate": "^0.5.9",
    "@fullstack-one/cli": "^0.5.11",
    "@fullstack-one/db": "^0.5.9",
    "@fullstack-one/di": "^0.5.2",
    "@fullstack-one/events": "^0.5.9",
    "@fullstack-one/file-storage": "^0.5.11",
    "@fullstack-one/graceful-shutdown": "^0.5.9",
    "@fullstack-one/graphql": "^0.5.9",
    "@fullstack-one/notifications": "^0.5.9",
    "@types/graphql": "^14.0.5",
    "apollo-server": "^2.3.1",
    "dotenv": "^4.0.0",
    "fullstack-one": "^0.5.5",
    "graphql": "^14.1.1",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0"
  }
}
