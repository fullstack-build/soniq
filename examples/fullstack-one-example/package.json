{
  "name": "fullstack.one-example",
  "version": "0.7.8",
  "description": "Example project: how to use fullstack.one",
  "private": true,
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "main": "index.ts",
  "scripts": {
    "start": "ts-node .",
    "debug": "FULLSTACK_ONE_DEV=true ts-node-dev --notify=false --inspect=9229 .",
    "dev": "FULLSTACK_ONE_DEV=true ts-node-dev --notify=false .",
    "dev-pure": "FULLSTACK_ONE_DEV=true ts-node .",
    "migrate-db": "one migrate-db",
    "generate-ts": "one generate-ts --output types/generated.ts",
    "tslint": "tslint './server/**/*.ts?(x)' -e '**/node_modules/**' --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fullstack-build/fullstack.one-example.git"
  },
  "keywords": [
    "fullstack.one",
    "example"
  ],
  "author": "Eugene <opensource@terehov.de> (http://fullstack.build)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fullstack-build/fullstack.one-example/issues"
  },
  "homepage": "https://github.com/fullstack-build/fullstack.one-example#readme",
  "devDependencies": {
    "@types/dotenv": "^4.0.2",
    "@types/node": "^8.10.51",
    "ts-node": "^4.1.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@fullstack-one/auth": "^0.7.8",
    "@fullstack-one/auth-fb-token": "^0.7.8",
    "@fullstack-one/db": "^0.7.8",
    "@fullstack-one/di": "^0.7.0",
    "@fullstack-one/events": "^0.7.8",
    "@fullstack-one/file-storage": "^0.7.8",
    "@fullstack-one/graceful-shutdown": "^0.7.8",
    "@fullstack-one/graphql": "^0.7.8",
    "@fullstack-one/notifications": "^0.7.8",
    "@types/graphql": "^14.2.2",
    "apollo-server": "^2.7.0",
    "dotenv": "^4.0.0",
    "fullstack-one": "^0.7.8",
    "graphql": "^14.4.2",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0"
  }
}
